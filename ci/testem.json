{
  "timeout": 600,
  "parallel": 5,
  "disable_watching": true,
  "on_start": "./ci/saucie-connect.js",
  "on_exit": "./ci/saucie-disconnect.js",
  "port": 7000,
  "browser_start_timeout": 90,
  "serve_files": [
    "test/fixtures/test_script.js",
    "dist/fabric.js",
    "test/unit/*.js"
  ],
  "test_page": "tests.mustache?hidepassed",
  "launchers": {
    "Node": {
      "command": "node test.js"
    },
    "SL_Chrome": {
      "exe": "node_modules/.bin/saucie",
      "args": ["-b", "chrome", "-v", "latest", "-p", "Windows 10", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_Firefox": {
      "exe": "node_modules/.bin/saucie",
      "args": ["-b", "firefox", "-v", "latest", "-p", "Windows 10", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_Safari": {
      "exe": "node_modules/.bin/saucie",
      "args": ["-b", "safari", "-v", "9.0", "-p", "OS X 10.11", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_MS_Edge": {
      "exe": "node_modules/.bin/saucie",
      "args": ["-b", "MicrosoftEdge", "-v", "latest", "-p", "Windows 10", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_IE_11": {
      "exe": "node_modules/.bin/saucie",
      "args": ["-b", "internet explorer", "-v", "11", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_IE_10": {
      "exe": "node_modules/.bin/saucie",
      "args": ["-b", "internet explorer", "-v", "10", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    },
    "SL_IE_9": {
      "exe": "node_modules/.bin/saucie",
      "args": ["-b", "internet explorer", "-v", "9", "--no-ct", "--at", "-u"],
      "protocol": "browser"
    }
  },
  "launch_in_dev": ["PhantomJS"],
  "launch_in_ci": [
    "PhantomJS",
    "SL_Chrome_Current",
    "SL_Chrome_Current_No_Details",
    "SL_Firefox_Current",
    "SL_Safari_Current",
    "SL_Safari_Last",
    "SL_MS_Edge",
    "SL_IE_11",
    "SL_IE_10",
    "SL_IE_9",
    "SL_IE_8"
  ]
}
